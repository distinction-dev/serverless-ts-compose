{
  "name": "serverless-typescript-servicea",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript boilerplate for service",
  "main": "serverless.ts",
  "scripts": {
    "start": "serverless offline --httpPort=4000",
    "deploy:staging": "npx sls deploy --aws-profile oneforce_uat --stage uat",
    "deploy:production": "serverless deploy --aws-profile oneforce_uat --stage prod",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --coverage",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prettier": "prettier --write '*/**/*.{js,ts}'",
    "docs": "redoc-cli serve docs/openapi/openapi.yml",
    "docs:watch": "redoc-cli serve docs/openapi/openapi.yml --watch"
  },
  "dependencies": {
    "@middy/core": "^4.2.6",
    "@middy/http-json-body-parser": "^4.2.6",
    "serverless-ssm-publish": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@serverless/compose": "^1.3.0",
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "commitlint": "^11.0.0",
    "esbuild": "^0.17.11",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.7",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.4.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "redoc-cli": "^0.13.2",
    "serverless": "3.27.0",
    "serverless-esbuild": "^1.39.0",
    "serverless-offline": "^12.0.4",
    "serverless-prune-plugin": "^1.5.0",
    "serverless-schema": "^1.17.0",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5"
  },
  "author": "Jeel Tilva",
  "license": "MIT"
}
